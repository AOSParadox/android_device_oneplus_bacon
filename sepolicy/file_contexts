# touchscreen gestures
/proc/touchpanel(/.*)?	                        u:object_r:proc_touchpanel:s0
/sys/class/leds/button-backlight/brightness	u:object_r:proc_touchpanel:s0

# We have a couple of non-standard NV partitions
/dev/block/bootdevice/by-name/oppodycnvbk          u:object_r:modem_efs_partition_device:s0
/dev/block/bootdevice/by-name/oppostanvbk          u:object_r:modem_efs_partition_device:s0

# Camera
/data/cam_socket.*                                 u:object_r:camera_socket:s0

# Wifi
/persist/.genmac                                u:object_r:wifi_data_file:s0
/persist/.bt_nv.bin                             u:object_r:bluetooth_data_file:s0

# Filesystems
/dev/block/bootdevice/by-name/boot         u:object_r:boot_block_device:s0
/dev/block/bootdevice/by-name/system       u:object_r:system_block_device:s0
/dev/block/bootdevice/by-name/recovery     u:object_r:recovery_block_device:s0
/dev/block/bootdevice/by-name/userdata     u:object_r:userdata_block_device:s0
/dev/block/bootdevice/by-name/metadata     u:object_r:metadata_block_device:s0
/dev/block/bootdevice/by-name/cache        u:object_r:cache_block_device:s0
